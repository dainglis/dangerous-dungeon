<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <engine:VisualElement name="container" style="flex-grow: 1;">
        <engine:VisualElement name="mainBackground" style="flex-grow: 1; position: absolute; top: 0; left: 0; right: 0; bottom: 0; background-color: rgba(0, 0, 0, 0.43);" />
        <engine:VisualElement name="actions" style="flex-grow: initial; flex-direction: row; justify-content: center; flex-shrink: 1; align-items: center; margin-top: auto; margin-bottom: 12px;">
            <engine:Button text="Cancel" name="buttonCancel" />
            <engine:Button text="Confirm" name="buttonConfirm" />
        </engine:VisualElement>
    </engine:VisualElement>
</engine:UXML>
